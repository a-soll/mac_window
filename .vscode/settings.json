{
    "files.associations": {
        "*.h": "c",
        "cstring": "c"
    },
    "code-runner.executorMapByGlob": {
        "*.c|*.h": "cd $workspaceRoot && clang -o $workspaceRoot/a.out $workspaceRoot/main.c $workspaceRoot/types/nsstring.c $workspaceRoot/types/nsarray.c $workspaceRoot/types/nsrunningapp.c $workspaceRoot/types/nsworkspace.c -framework AppKit && $workspaceRoot/a.out"
    }
}
