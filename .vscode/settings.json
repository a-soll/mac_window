{
    "files.associations": {
        "*.h": "c",
        "cstring": "c",
        "array": "c",
        "deque": "c",
        "vector": "c",
        "string_view": "c",
        "ranges": "c",
        "thread": "c"
    },
    "code-runner.executorMapByGlob": {
        "main.c": "cd $workspaceRoot && clang -o $workspaceRoot/a.out main.c $workspaceRoot/source/windows.c /usr/local/lib/libobjc.a -framework AppKit && $workspaceRoot/a.out",
        "(spaces|secret).c": "cd /Users/adam/code/mac_window && clang -o /Users/adam/code/mac_window/using_secret/a.out $fullFileName /usr/local/lib/libobjc.a -framework AppKit -iframework /System/Library/PrivateFrameworks -framework SkyLight && /Users/adam/code/mac_window/using_secret/a.out"
    }
}
